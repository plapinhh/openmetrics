<h1>Listing service_histories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Typ</th>
    <th>Group</th>
    <th>Description</th>
    <th>Service</th>
    <th>Running from</th>
    <th>Running to</th>
  </tr>

<% for service_history in @service_histories %>
  <tr>
    <td><%=h service_history.name %></td>
    <td><%=h service_history.typ %></td>
    <td><%=h service_history.group %></td>
    <td><%=h service_history.description %></td>
    <td><%=h service_history.service_id %></td>
    <td><%=h service_history.running_from %></td>
    <td><%=h service_history.running_to %></td>
    <td><%= link_to 'Show', service_history %></td>
    <td><%= link_to 'Edit', edit_service_history_path(service_history) %></td>
    <td><%= link_to 'Destroy', service_history, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New service_history', new_service_history_path %>
